<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.com.bdo.taxdg.dao.TaxDgWorkPaperDao">
    <select id="queryEntity" parameterType="cn.com.bdo.taxdg.dto.projectset.TaxDgWorkPaperDto"
            resultType="cn.com.bdo.taxdg.entity.TaxDgWorkPaperEntity">
        select
        *
        from
            bdo_taxdg.tax_dg_workpaper
        where
            projectId = #{projectId}
        and
            fileIndexId = #{fileIndexId}
    </select>

</mapper>
